							<div class="icon">
								<i class="icon">
									{{ content.svg_blocl1 }}
								</i>
							</div>
							<div class="services-content">
								<h3>
									{{ content.title_blocl1 }}
								</h3>
								<div class="description">{{ content.desc_blocl1 }}</div>
							</div>
						</div>
					</div>
					<div class="col-md-6 col-lg-4 col-xs-12">
