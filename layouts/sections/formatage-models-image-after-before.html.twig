{% if content %}
	<div{{attributes.addClass('main-twentytwenty')}}>
		<div {% if settings.config_section %} class="{{ settings.config_section.type_container }} {{ settings.config_section.container_class }}" {% else %} class="container" {% endif %}>
			<div class="d-flex justify-content-center buttons">
				<div{{region_attributes.button1.addclass(['seeMore','after-before']).setattribute('data-tab','#tab-vue-twentytwenty')}}>{{ content.button1 }}</div>
				<div{{region_attributes.button2.addclass(['seeMore','vue-generale']).setattribute('data-tab','#tab-vue-general')}}>{{ content.button2 }}</div>
			</div>
			<div class="tab-content">
				<div class="container-zurb-twentytwenty tab-pane fade show active" role="tabpanel" id="tab-vue-twentytwenty">
					<div class="zurb-twentytwenty">
						{{ content.image1|layout_value }}
						{{ content.image2|layout_value  }}
					</div>
				</div>
				<div {{ region_attributes.image.addClass( ['container-vue-general', 'tab-pane', 'fade'] ) }} role="tabpanel" id="tab-vue-general">
					{{ content.image }}
				</div>
			</div>
		</div>
	</div>
{% endif %}