{% if content %}
	<section{{attributes.addClass(['fm-header-thegem-01'])}}>
		<div class="container">
			<div class="row align-items-center">
				<div class="col">
					<div class="row align-items-center">
						{% for item in settings.infos.fields if item['text'] %}
							<div class="col-auto item">
								<i class="fas fa-map-marker-alt"></i>
								monsdsljhl dxfdf
							</div>
						{% endfor %}
						<div class="col item">
							<div class="d-flex socials-items">
								{% for item in settings['social-items'].fields  %}
									<a class="socials-item" target="_blank">
										<i class="fab fa-instagram"></i>
									</a>
								{% endfor %}
							</div>
						</div>
					</div>
				</div>
				<div class="col-md-auto">
					<div class="row align-items-center link-text">
						{% for item in settings.links.fields  %}
							<div class="col angle-right d-flex align-items-center item">
								<a href="#" class="link">
									helllll ooooohh
								</a>
							</div>
						{% endfor %}
						<div class="col d-flex align-items-center">
							<a class="btn btn-info'">
								Join now
							</a>
						</div>
					</div>
				</div>
			</div>
		</div>
	</section>
{% endif %}